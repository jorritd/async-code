{
  "name": "asynccode",
  "version": "0.0.3",
  "description": "Make a code block async, overcome the 'max call stack exceeded' error",
  "main": "asynccode.js",
  "scripts": {
    "test": "node test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorritd/async-code"
  },
  "keywords": [
    "json",
    "compress"
  ],
  "author": "Jorrit Duin",
  "license": "ISC",
  "homepage": "https://github.com/jorritd/async-code",
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "amd": true,
      "node": true
    },
    "globals": {
    },
    "extends": "eslint:recommended",
    "plugins": [
      "security"
    ]
  },
  "dependencies": {
    "requirejs": "^2.3.1"
  }
}
